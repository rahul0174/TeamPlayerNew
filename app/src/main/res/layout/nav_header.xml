                android:layout_height="match_parent"
                android:orientation="vertical"
                >
                <RelativeLayout
                    android:id="@+id/rl_sign_in"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_40sdp"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_20sdp"
                    android:background="@drawable/rounded_blue_btn"
                    >
                    <com.cts.teamplayer.customui.CustomTextviewBold
                        android:id="@+id/tv_signin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:text="@string/sign_in"
                        android:padding="@dimen/_8sdp"
                        android:layout_centerInParent="true"
                        android:textSize="@dimen/_12sdp"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_sign_up"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_20sdp"
                    android:background="@drawable/rounded_corner_blue"
                    >
                    <com.cts.teamplayer.customui.CustomTextviewBold
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/light_blue"
                        android:text="@string/signup"
                        android:padding="@dimen/_8sdp"
                        android:layout_centerInParent="true"
                        android:textSize="@dimen/_12sdp"
                        />
                </RelativeLayout>
                <com.cts.teamplayer.customui.CustomTextviewBold
                    android:id="@+id/tv_open_home"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:textAlignment="center"
                    android:padding="@dimen/_7sdp"
                    android:layout_marginTop="@dimen/_35sdp"
                    android:textSize="@dimen/_11sdp"
                    android:text="@string/home"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1ssp"
                    android:background="@color/light_grey_new"
                    />
                <com.cts.teamplayer.customui.CustomTextviewBold
                    android:id="@+id/tv_open_how_work"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:textAlignment="center"
                    android:padding="@dimen/_7sdp"
                    android:textStyle="bold"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:textSize="@dimen/_11sdp"
                    android:text="@string/how_it_works"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1ssp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="@color/light_grey_new"
                    />
                <com.cts.teamplayer.customui.CustomTextviewBold
                    android:id="@+id/tv_open_vision_tech"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:padding="@dimen/_7sdp"
                    android:layout_marginTop="@dimen/_7sdp"
